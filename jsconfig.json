{
  "typeAcquisition": {
    "exclude": []
  },
  "allowJS": true,
  "compilerOptions": {
    "module": "ES6",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "exclude": ["node_modules"]
}
